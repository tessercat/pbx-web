<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<document type="freeswitch/xml">
  <section name="directory">
    <domain name="{{ directory_domain }}">
      <user id="{{ user_id }}">
        <params>
          <param name="jsonrpc-allowed-methods" value="{{ allowed_methods|join:',' }}" />
          <param name="jsonrpc-allowed-event-channels" value="{{ allowed_event_channels|join:',' }}" />
          <param name="password" value="{{ password }}" />
        </params>
      </user>
    </domain>
  </section>
</document>
