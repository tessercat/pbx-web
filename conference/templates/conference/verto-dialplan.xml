<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<document type="freeswitch/xml">
  <section name="dialplan">
    <context name="verto">
      <extension name="Conference">
        <condition regex="all">
          <regex field="destination_number" expression="^{{ dest }}$"/>
          <action application="conference" data="{{ confname }}"/>
        </condition>
      </extension>
    </context>
  </section>
</document>
