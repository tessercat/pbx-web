<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<document type="freeswitch/xml">
  <section name="directory">
    <domain name="verto">
      <user id="{{ client_id }}">
        <params>
          <param name="jsonrpc-allowed-methods" value="{{ extension.methods|join:',' }}"/>
          <param name="password" value="{{ password }}"/>
        </params>
      </user>
    </domain>
  </section>
</document>
